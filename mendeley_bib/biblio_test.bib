issn = {1470-269X},
journal = {The pharmacogenomics journal},
keywords = {analogs and derivatives,camptothecin,fluorouracil,glucuronosyltransferase,methylenetetrahydrofolate reductase,nadph2,p-glycoprotein,thymidylate synthase},
number = {1},
