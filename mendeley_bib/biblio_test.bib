journal = {Oncotarget},
keywords = {FBW7,chemotherapy,esophageal squamous cell carcinoma,paclitaxel,single nucleotide polymorphisms},
number = {28},
pages = {44330--44339},
pmid = {27259248},
title = {{Study of single nucleotide polymorphisms of FBW7 and its substrate genes revealed a predictive factor for paclitaxel plus cisplatin chemotherapy in Chinese patients with advanced esophageal squamous cell carcinoma.}},
