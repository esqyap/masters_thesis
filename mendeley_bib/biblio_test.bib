issn = {0960-314X},
journal = {Pharmacogenetics},
keywords = {80 and over,Adult,Aged,Antimetabolites,Antineoplastic/ therapeutic use,Colorectal Neoplasms/ drug therapy/ genetics/secon,European Continental Ancestry Group,Female,Fluorouracil/ therapeutic use,France,Genetic,Heterozygote,Homozygote,Humans,Male,Methylenetetrahydrofolate Reductase (NADPH2)/ gene,Middle Aged,Neoplasm Metastasis/drug therapy,Neoplasm Staging,Pharmacogenetics,Polymorphism,Prognosis,Retrospective Studies,Risk,Survival Rate},
