issn = {03014851},
journal = {Molecular Biology Reports},
keywords = {Colorectal cancer,MDM2,Polymorphism,TP53},
number = {10},
pages = {9661--9668},
pmid = {22744426},
title = {{Polymorphisms in TP53 and MDM2 contribute to higher risk of colorectal cancer in Chinese population: A hospital-based, case-control study}},
