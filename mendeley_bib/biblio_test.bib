isbn = {0007-0920 (Print)$\backslash$r0007-0920 (Linking)},
issn = {0007-0920},
journal = {British Journal of Cancer},
keywords = {1999,2000,2001,and dna-repair enzymes,ando et al,colorectal cancer,enzymes,for drug efficacy,gebhardt et al,lima et al,may have important implications,metabolising,one of,park et al,pharmacogenetics,platinum therapy,polymorphism,target genes},
