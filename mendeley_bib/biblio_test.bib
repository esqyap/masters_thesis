journal = {Briefings in Bioinformatics},
keywords = {Alignment algorithm,New sequencing technologies,Sequence analysis},
number = {5},
pages = {473--483},
pmid = {20460430},
title = {{A survey of sequence alignment algorithms for next-generation sequencing}},
volume = {11},
