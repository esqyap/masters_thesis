eprint = {/www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3006164{\&}tool=pmcentrez{\&}rendertype=abstract.},
file = {:Users/evayap/Library/Application Support/Mendeley Desktop/Downloaded/Chung et al. - 2016 - The minimal amount of starting DNA for Agilent's hybrid capture-based targeted massively parallel sequencing.pdf:pdf},
isbn = {4936551015},
issn = {2045-2322},
journal = {Scientific Reports},
number = {1},
