doi = {10.1158/1078-0432.CCR-10-2209},
file = {:Users/evayap/Library/Application Support/Mendeley Desktop/Downloaded/Deenen et al. - 2011 - Relationship between single nucleotide polymorphisms and haplotypes in DPYD and toxicity and efficacy of capecita.pdf:pdf},
