issn = {1530-6860},
journal = {FASEB journal : official publication of the Federation of American Societies for Experimental Biology},
keywords = {Acetylcholine,Acetylcholine: pharmacology,Animals,Antigens,Arterioles,Arterioles: cytology,Arterioles: metabolism,CD31,CD31: genetics,CD31: metabolism,Endothelium,Female,Gene Deletion,Guanylate Cyclase,Guanylate Cyclase: antagonists {\&} inhibitors,Guanylate Cyclase: metabolism,Histamine,Histamine: pharmacology,Inbred C57BL,Knockout,Male,Mice,Muscle,Nitric Oxide,Nitric Oxide Synthase,Nitric Oxide Synthase Type II,Nitric Oxide Synthase Type III,Nitric Oxide Synthase: deficiency,Nitric Oxide Synthase: genetics,Nitric Oxide Synthase: metabolism,Nitric Oxide: metabolism,Skeletal,Skeletal: blood supply,Vascular,Vascular: drug effects,Vascular: enzymology,Vascular: metabolism,Vasodilation,Vasodilation: drug effects},
