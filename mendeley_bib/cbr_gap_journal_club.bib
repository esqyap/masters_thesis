@article{Payne2004,
abstract = {Conducted vasodilation along arterioles manifests the spread of hyperpolarization through gap junction channels along endothelium. Whereas histamine increases the permeability of capillary and venular endothelium, its effect on the integrity of arteriolar endothelium is unknown. We tested whether histamine could inhibit conducted vasodilation. In second-order arterioles (2A) supplying the cremaster muscle of C57BL6, PECAM-1-/-, and eNOS-/- mice (8-12 wk), neither resting (16+/-2 microm) nor maximal (38+/-2 microm) diameters were different. Acetylcholine (ACh) microiontophoresis (1 microA, 500 ms pulse) triggered vasodilation that was conducted {\textgreater}1400 microm along arterioles. Neither local (14+/-2 microm) nor conducted vasodilation (10+/-2 microm) was different among mice. Histamine (5 microM) had no effect on resting diameter or local vasodilation to ACh yet inhibited conduction by {\textgreater}50{\%} in C57BL6 and PECAM-1-/- mice (P{\textless}0.05); this effect was abolished after blockade of NO synthase or of soluble guanylate cyclase. Washout of histamine restored conduction, though recovery was longer (P{\textless}0.05) in PECAM-1-/- mice vs. C57BL6 mice. Remarkably, conducted vasodilation in eNOS-/- mice was insensitive to histamine. These findings indicate that histamine inhibits cell-to-cell coupling through an NO-dependent mechanism and suggests a dynamic interaction among intercellular adhesion molecules and gap junction channels along arteriolar endothelium.},
author = {Payne, Geoffrey W and Madri, Joseph A and Sessa, William C and Segal, Steven S},
doi = {10.1096/fj.03-0752com},
file = {:Users/evayap/Library/Application Support/Mendeley Desktop/Downloaded/Payne et al. - 2004 - Histamine inhibits conducted vasodilation through endothelium-derived NO production in arterioles of mouse skeleta.pdf:pdf},
