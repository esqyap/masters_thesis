file = {:Users/evayap/Library/Application Support/Mendeley Desktop/Downloaded/Peyrl et al. - 2003 - Protein profiles of medulloblastoma cell lines DAOY and D283 Identification of tumor-related proteins and principl.pdf:pdf},
isbn = {1615-9853 (Print)1615-9853},
issn = {16159853},
journal = {Proteomics},
keywords = {D283,DAOY,Medulloblastoma},
number = {9},
pages = {1781--1800},
