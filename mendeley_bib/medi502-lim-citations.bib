title = {{Phase II study of cilengitide in the treatment of refractory or relapsed high-grade gliomas in children: A report from the Children's Oncology Group}},
url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-84884998198{\&}partnerID=40{\&}md5=a0d9e6c2000a47e9cfc8336e6c7b65df},
