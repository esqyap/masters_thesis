file = {:Users/evayap/Library/Application Support/Mendeley Desktop/Downloaded/Packer et al. - 2006 - Phase III study of craniospinal radiation therapy followed by adjuvant chemotherapy for newly diagnosed average-r.pdf:pdf},
isbn = {1527-7755 (Electronic)$\backslash$r0732-183X (Linking)},
