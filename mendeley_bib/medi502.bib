file = {:Users/evayap/Library/Application Support/Mendeley Desktop/Downloaded/Milde et al. - 2012 - HD-MB03 is a novel Group 3 medulloblastoma model demonstrating sensitivity to histone deacetylase inhibitor treatm.pdf:pdf},
isbn = {1573-7373 (Electronic)$\backslash$r0167-594X (Linking)},
issn = {0167594X},
