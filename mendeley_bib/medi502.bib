isbn = {0305-1846 (Print)$\backslash$r0305-1846 (Linking)},
issn = {03051846},
journal = {Neuropathology and Applied Neurobiology},
keywords = {Age,Apoptosis,Cerebellum,Desmoplastic,Immunohistochemistry,Prognosis,Survival,Synaptophysin},
number = {5},
pages = {513--521},
pmid = {15488027},
title = {{Bcl-2 expression inversely correlates with tumour cell differentiation in medulloblastoma}},
