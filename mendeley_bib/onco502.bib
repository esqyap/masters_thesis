journal = {Cancer discovery},
keywords = {Animals,Humans,Immunotherapy, Adoptive,Neoplasms,Neoplasms: therapy,Receptors, Antigen,Receptors, Antigen: metabolism,Recombinant Fusion Proteins,Recombinant Fusion Proteins: metabolism,Signal Transduction,T-Lymphocytes,T-Lymphocytes: immunology},
