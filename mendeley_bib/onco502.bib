file = {:Users/evayap/Library/Application Support/Mendeley Desktop/Downloaded/Grupp et al. - 2013 - Chimeric antigen receptor-modified T cells for acute lymphoid leukemia.pdf:pdf},
issn = {1533-4406},
journal = {The New England journal of medicine},
keywords = {Antigen,Antigens,CD19,Child,Chimera,Female,Humans,Immunotherapy,Precursor Cell Lymphoblastic Leukemia-Lymphoma,Precursor Cell Lymphoblastic Leukemia-Lymphoma: im,Precursor Cell Lymphoblastic Leukemia-Lymphoma: th,Receptors,Remission Induction,T-Cell,T-Lymphocytes,T-Lymphocytes: immunology},
month = {apr},
