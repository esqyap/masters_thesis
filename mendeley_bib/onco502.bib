file = {:Users/evayap/Library/Application Support/Mendeley Desktop/Downloaded/Sarantopoulos et al. - 2007 - High levels of B-cell activating factor in patients with active chronic graft-versus-host disease.pdf:pdf},
issn = {1078-0432},
