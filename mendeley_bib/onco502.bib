file = {:Users/evayap/Library/Application Support/Mendeley Desktop/Downloaded/Spangle, M{\"{u}}nger - 2010 - The human papillomavirus type 16 E6 oncoprotein activates mTORC1 signaling and increases protein synthesis.pdf:pdf},
