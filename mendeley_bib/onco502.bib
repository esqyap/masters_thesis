journal = {Frontiers in pharmacology},
keywords = {as hypoxia and acidic,cox-2 and,cox-2 and hif-1,cox-2 inhibitors,cyclooxygenase 2,cyclooxygenase 2, inflammation and cancer, inflamm,hostile physiological environments such,inflammation and cancer,inflammation and hypoxia,inflammation in the tumor,invasion,lef-1,metastasis,microenvironment},
month = {jan},
